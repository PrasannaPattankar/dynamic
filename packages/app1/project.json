{
  "name": "app1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/app1/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rsbuild:rsbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/app1",
        "config": "packages/app1/rsbuild.config.ts"
      },
      "configurations": {
        "development": {
          "watch": true
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/rsbuild:dev-server",
      "options": {
        "buildTarget": "app1:build:development",
        "port": 4201
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "app1:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/app1/**/*.ts", "packages/app1/**/*.tsx"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/app1/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
