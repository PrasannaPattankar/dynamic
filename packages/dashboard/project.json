{
  "name": "dashboard",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dashboard/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rsbuild:rsbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/dashboard",
        "config": "packages/dashboard/rsbuild.config.ts"
      },
      "configurations": {
        "development": {
          "watch": true
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/rsbuild:dev-server",
      "options": {
        "buildTarget": "dashboard:build:development",
        "port": 4200
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "dashboard:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/dashboard/**/*.ts", "packages/dashboard/**/*.tsx"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/dashboard/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
